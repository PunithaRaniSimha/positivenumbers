NumList = []

Number = int(input("Please enter the Total Number of List1 Element:"))
for i in range(1, Number + 1):
    value = int(input("Please enter the value of %d Element : " %i))
    NumList.append(value)

print("\nPositive Number in this List1 are : ")
for j in range(Number):
    if(NumList[j] >= 0):
        print(NumList[j], end = '  ')

Number = int(input("Please enter the Total Number of List2 Element:"))
for i in range(1, Number + 1):
    value = int(input("Please enter the value of %d Element : " % i))
    NumList.append(value)

print("\nPositive Number in this List2 are : ")
for j in range(Number):
    if (NumList[j] >= 0):
        print(NumList[j], end='  ')
